    <div class="quemsomos row">
        <div data-aos="fade-right" data-aos-duration="3000" class="col-md-6 quemsomos__col">
            <div class="quemsomos__content" data-aos="fade-right" data-aos-duration="3000">
                <h1 class="quemsomos__content__title">OUR</h1>
                <h2 class="quemsomos__content__subtitle">HISTORY</h2>
